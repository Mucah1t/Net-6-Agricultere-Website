<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_HeadPartial")  @*to call our ViewComponent*@

<body>
	<!-- header -->
	<div class="header">
		<div class="container">
@await Component.InvokeAsync("_TopAddressPartial")  @*to call our ViewComponent*@
			@await Component.InvokeAsync("_SocialMediaPartial")  @*to call our ViewComponent*@

			<div class="clearfix"> </div>
		</div>
	</div>

	<!-- //header -->
	<!--Slider-->
	@await Component.InvokeAsync("_NavbarPartial")  @*to call our ViewComponent*@


	<div class="modal fade" id="myModal1" tabindex="-1" role="dialog">
		<!-- Modal1 -->
		<div class="modal-dialog">
			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4>In Harvest</h4>
					<img src="~/web/images/banner1.jpg" alt=" " class="img-responsive">
					<h5>ENVIRONMENT & SUSTAINABILITY</h5>
					<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries, but also the leap into electronic typesetting.</p>
				</div>
			</div>
		</div>
	</div>
	
	<!-- about -->
	@await Component.InvokeAsync("_AboutPartial")  @*to call our ViewComponent*@

	
	<!--sevices-->
	@await Component.InvokeAsync("_ServicePartial")  @*to call our ViewComponent*@

	<!--//sevices-->
	<!-- team -->
	@await Component.InvokeAsync("_TeamPartial")  @*to call our ViewComponent*@

	<!-- //team -->
	<!--blog -->
	@await Component.InvokeAsync("_AnnouncementPartial")  @*to call our ViewComponent*@

	<!-- //blog -->

	<!-- //Modal2 -->

	<!-- //Modal4 -->
	<!--/gallery-->
	@await Component.InvokeAsync("_GalleryPartial")  @*to call our ViewComponent*@

	<!--//gallery-->
	<!--footer-->
	<div class="contact-agile" id="contact">
		<div class="container">
			@await Component.InvokeAsync("_AddressPartial")  @*to call our ViewComponent*@

		
			@await Html.PartialAsync("/Views/Default/SendMessage.cshtml")
		</div>
	</div>
	@await Component.InvokeAsync("_MapPartial")  @*to call our ViewComponent*@
	<div class="bottom-social-w3ls w3ls-social-icons">
		<a class="facebook" href="#"><i class="fa fa-facebook"></i></a>
		<a class="twitter" href="#"><i class="fa fa-twitter"></i></a>
		<a class="pinterest" href="#"><i class="fa fa-pinterest-p"></i></a>
		<a class="linkedin" href="#"><i class="fa fa-linkedin"></i></a>
	</div>
	<div class="copy">
		<p>© @DateTime.Now.Year In Harvest . All Rights Reserved | Design by <a href="http://w3layouts.com/">W3layouts</a> </p>
	</div>
	<!--/footer -->
	<!-- js -->
	@await Html.PartialAsync("/Views/Default/ScriptPartial.cshtml")


</body>
</html>
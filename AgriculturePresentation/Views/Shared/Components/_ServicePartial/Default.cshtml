@using EntityLayer.Concrete
@model List<Service>


<div class="sevices-w3layouts" id="services">
	<div class="container">
		<h3 class="tittle">Our Services</h3>
		<p class="heading-btm-w3ls">The services we provide to you in different areas..</p>
		<div class="services-main">
			@foreach (var item in Model)
			{
				
		
			<div class="@item.Class1" data-wow-delay=".5s">
				<h5>@item.Description</h5>
				<p>Lorem ipsum dolor sit amet, consectetur adipi est eligendi scing elit consectetur</p>

				<div class="@item.Class2">
					<i class="@item.Image" aria-hidden="true"></i>
					<p>@item.Title</p>
				</div>
			</div>
			}
			
		</div>
		<div class="clearfix"> </div>
	</div>
</div>